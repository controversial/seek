{
  "name": "seek",
  "version": "0.0.1",
  "description": "A proof-of-concept implementation of an automated school attendance system",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --inline"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/controversial/seek.git"
  },
  "author": "Luke Deen Taylor",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/controversial/seek/issues"
  },
  "homepage": "https://github.com/controversial/seek#readme",
  "devDependencies": {
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "^2.1.1",
    "dotenv": "^8.0.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-vue": "^5.2.2",
    "extract-loader": "^3.1.0",
    "file-loader": "^4.0.0",
    "html-loader": "^0.5.5",
    "node-sass": "^4.13.1",
    "sass-lint": "^1.13.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "url-loader": "^2.0.0",
    "vue-loader": "^15.7.0",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.3",
    "webpack-dev-server": "^3.7.1"
  },
  "dependencies": {
    "apollo-boost": "^0.4.2",
    "graphql": "^14.3.1",
    "graphql-tag": "^2.10.1",
    "vue": "^2.6.10",
    "vue-apollo": "^3.0.0-beta.30",
    "vue-feather-icons": "^4.22.0",
    "vue-router": "^3.0.6",
    "vuex": "^3.1.1"
  },
  "eslintConfig": {
    "extends": [
      "airbnb",
      "plugin:vue/essential"
    ],
    "env": {
      "browser": true
    },
    "rules": {
      "import/no-extraneous-dependencies": [
        "error",
        {
          "devDependencies": true
        }
      ],
      "no-param-reassign": [
        "error",
        {
          "props": false
        }
      ],
      "no-underscore-dangle": "off",
      "global-require": "off",
      "object-curly-newline": [
        "error",
        {
          "consistent": true
        }
      ],
      "import/no-webpack-loader-syntax": "off",
      "import/newline-after-import": "off",
      "react/no-this-in-sfc": "off"
    },
    "settings": {
      "import/resolver": "webpack"
    },
    "plugins": [
      "vue"
    ]
  }
}
